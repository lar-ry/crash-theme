{
  "name": "crash-theme",
  "displayName": "Crash Theme",
  "description": "%description%",
  "repository": "https://github.com/lar-ry/crash-theme",
  "publisher": "larry-lan",
  "icon": "assets/logo.png",
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "version": "0.1.6",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "black",
    "dark",
    "modern",
    "theme"
  ],
  "contributes": {
    "themes": [
      {
        "label": "%crash_dark%",
        "uiTheme": "vs-dark",
        "description": "%crash_dark_description%",
        "path": "./themes/crash-dark-color-theme.json"
      },
      {
        "label": "%crash_green%",
        "uiTheme": "vs-dark",
        "description": "%crash_green_description%",
        "path": "./themes/crash-green-color-theme.json"
      }
    ]
  }
}